    "import numpy as np\n",
    "np.random.seed(5)\n",
    "def daoshu1(arr):\n",
    "    output = np.empty(len(arr))\n",
    "    for i in range(len(arr)):\n",
    "        output[i] = 1.0 / arr[i]\n",
    "    return output\n",
    "arr = np.random.randint(1,10,size=6)\n",
    "daoshu1(arr)"
   ]
